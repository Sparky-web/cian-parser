{
  "kind": "collectionType",
  "collectionName": "links",
  "info": {
    "name": "Links",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "url": {
      "type": "string",
      "required": true
    },
    "mode": {
      "type": "enumeration",
      "enum": [
        "parse_all",
        "parse_first_pages"
      ],
      "default": "parse_first_pages",
      "required": true
    },
    "responsible": {
      "type": "integer",
      "required": true
    },
    "isFirstParse": {
      "type": "boolean",
      "default": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "isEnabled": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "lastParse": {
      "type": "component",
      "repeatable": false,
      "component": "links.last-parse"
    },
    "frequency": {
      "type": "enumeration",
      "enum": [
        "every_minute",
        "every_5_minutes",
        "every_10_minutes",
        "every_15_minutes",
        "every_30_minutes",
        "every_60_minutes",
        "every_2_hours",
        "every_3_hours",
        "every_4_hours",
        "every_5_hours",
        "every_6_hours",
        "every_7_hours",
        "every_8_hours",
        "every_9_hours",
        "every_10_hours",
        "every_11_hours",
        "every_12_hours",
        "every_day"
      ],
      "default": "every_30_minutes",
      "required": false
    },
    "shouldAddToBitrix": {
      "type": "boolean",
      "default": true
    }
  }
}
